{
	"cli": {
		"version": ">= 12.1.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"GOOGLE_SERVICES_JSON": "./google-services.json"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"GOOGLE_SERVICES_JSON": "./google-services.json"
			},
			"image": "latest"
		},
		"production": {
			"autoIncrement": true
		}
	},
	"submit": {
		"production": {}
	}
}
